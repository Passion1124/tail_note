.success, .fail{
  width: 100%;
  padding: 133rpx 0 0;
  text-align: center;
  box-sizing: border-box
}

.fail{
  padding-top: 112rpx;
}

.success .title, .fail .title{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  margin-bottom: 24rpx;
}

.img_row{
  width: 52rpx;
  height: 52rpx;
  margin-right: 14rpx;
}

.img_row image{
  width: 100%;
  height: 100%;
}

.success .title .text, .fail .title .text{
  font-size: 56rpx;
  line-height: 78rpx;
  color: #23C2F7;
}

.fail .title .text{
  color: #F97572;
}

.success .subTitle, .fail .subTitle{
  font-size: 24rpx;
  line-height: 32rpx;
  color: #666666;
  margin-bottom: 80rpx;
}

.fail .subTitle{
  width: 288rpx;
  margin: 0 auto 80rpx;
}

.success .subTitle text text, .fail .subTitle text text{
  color: #F97572;
}

.success button{
  width: 320rpx;
  height: 64rpx;
  line-height: 64rpx;
  border-radius: 50rpx;
  font-size: 28rpx;
  letter-spacing: 2px;
  color: #3C3C3C;
  border: 1rpx solid #B7B7B7;
  background-color: #fff;
}

.fail .btn{
  display: flex;
  align-items: center;
  justify-content: center;
}

.fail button{
  width: 240rpx;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 28rpx;
  letter-spacing: 2px;
  color: #666666;
  border: 1rpx solid #B7B7B7;
  border-radius: 50rpx;
  background-color: #fff;
  margin: 0;
}

.fail button:first-of-type{
  color: #3C3C3C;
  margin-right: 40rpx;
}