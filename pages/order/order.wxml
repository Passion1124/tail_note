<!-- order.wxml -->

<view class='container'>
  <view class='banner'>
    <image class='banner_img' src='../../img/avatar.png'></image>
  </view>
  <view class='classify'>
    <view class='pengding'>
      <image class='icons'></image>
      <text>待付款</text>
    </view>
    <view class='paid'>
      <image class='icons'></image>
      <text>已付款</text>
    </view>
    <view class='completed'>
      <image class='icons'></image>
      <text>已完成</text>
    </view>
    <view class='cancelled'>
      <image class='icons'></image>
      <text>已取消</text>
    </view>
    <view class='refunded'>
      <image class='icons'></image>
      <text>已退款</text>
    </view>
  </view>
  <view class='list'>
    <view>
      <text>全部订单</text>
    </view>
    <view>
      <image src='../../img/more.png'></image>
    </view>
  </view>
  <view class='list'>
    <view>
      <text>我的收藏</text>
    </view>
    <view>
      <image src='../../img/more.png'></image>
    </view>
  </view>
</view>